{
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts"
        ]
    },
    "rules": {
        "semicolon": true,
        "quotemark": [
            true,
            "double"
        ],
        "no-trailing-whitespace": true
    }
}